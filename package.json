{
  "name": "html-chat",
  "version": "0.0.1",
  "description": "Simple chat with react.js",
  "main": "main.js",
  "scripts": {
    "test": "jest",
    "start": "cross-env NODE_ENV=development webpack-dev-server --config ./configs/webpack.development.js",
    "build": "cross-env NODE_ENV=production webpack --config ./configs/webpack.production.js",
    "lint": "eslint src/",
    "lint-fix": "eslint src/ --fix",
    "prettier": "prettier --write \"src/**/*.js\"",
    "storybook": "start-storybook -p 6006 --no-dll",
    "build-storybook": "build-storybook --no-dll",
    "loki:test": "npm run build-storybook && npx loki test --reactUri file:./storybook-static",
    "loki:update": "npm run build-storybook && npx loki update --reactUri file:./storybook-static",
    "loki:approve": "npx loki approve",
    "nightwatch": "nightwatch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jyabka/Chat_Name.git"
  },
  "keywords": [
    "chat",
    "react.js"
  ],
  "author": "Jyabka",
  "license": "none",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@material-ui/core": "^4.11.1",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@storybook/addon-actions": "^6.0.28",
    "@storybook/addon-essentials": "^6.0.28",
    "@storybook/addon-links": "^6.0.28",
    "@storybook/react": "^6.0.28",
    "axios": "^0.21.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "chromedriver": "^87.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.1.1",
    "cross-env": "^7.0.2",
    "css-loader": "^4.3.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-sonarjs": "^0.5.0",
    "eslint-plugin-standard": "^4.0.2",
    "eslint-webpack-plugin": "^2.4.0",
    "file-loader": "^6.1.0",
    "formik": "^2.2.2",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "lint-staged": "^10.4.2",
    "loki": "^0.25.1",
    "nightwatch": "^1.5.1",
    "prettier": "^2.1.2",
    "react-is": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-test-renderer": "^16.13.1",
    "style-loader": "^1.2.1",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.1.4"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "jest": {
    "setupFiles": [
      "./setupTests.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.(js|jsx)": [
      "eslint --fix"
    ]
  },
  "loki": {
    "configurations": {
      "chrome.laptop": {
        "target": "chrome.app",
        "width": 1366,
        "height": 768,
        "deviceScaleFactor": 1,
        "mobile": false
      },
      "chrome.iphone7": {
        "target": "chrome.app",
        "preset": "iPhone 7"
      }
    }
  }
}
